<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Application\Entity\Event" table="event">
        <id name="id" type="bigint" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="projectId" type="bigint" column="projectId" nullable="false" precision="0" scale="0"/>
        <field name="userId" type="bigint" column="userId" nullable="false" precision="0" scale="0"/>
        <field name="action" type="string" column="action" nullable="false" precision="0" scale="0"/>
        <field name="happenedOn" type="datetime" column="happenedOn" nullable="false" precision="0" scale="0"/>
    </entity>
</doctrine-mapping>
